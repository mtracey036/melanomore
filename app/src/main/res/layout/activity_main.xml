<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.melanomore.melanomore.MainActivity">

    <EditText
        android:id="@+id/editText4"
        android:layout_width="390dp"
        android:layout_height="59dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Enter Primary Care Physician's Name"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="355dp"
        android:layout_height="50dp"
        android:layout_marginRight="12dp"
        android:text="Enter Physician's Full Name"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="150dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="356dp"
        android:layout_height="69dp"
        android:layout_marginLeft="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Enter Physician's Email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="356dp"
        android:layout_height="59dp"
        android:layout_marginLeft="16dp"
        android:text="Enter Physician's Number"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="286dp" />
</android.support.constraint.ConstraintLayout>
